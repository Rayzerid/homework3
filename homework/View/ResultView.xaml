<Window x:Class="homework.View.ResultView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:homework.View"
        mc:Ignorable="d"
        Title="ResultView" Height="400" Width="500">
    <Grid>
        <DataGrid x:Name="studentInfo" ItemsSource="{Binding ResultStudent, UpdateSourceTrigger=PropertyChanged}" />
    </Grid>
</Window>
